{
  "name": "get_saved_posts",
  "version": "1.0.0",
  "description": "Download your saved posts from reddit into a easy to data mine format.",
  "main": "index.js",
  "scripts": {
    "start": "tsc && npm run copy-views-assets && node ./dist/server.js",
    "dev": "nodemon",
    "test": "jest --silent",
    "db": "ts-node -r tsconfig-paths/register -- ./src/script.ts",
    "watch-ts": "tsc -w",
    "build": "tsc && npm run copy-views-assets",
    "all": "npm run copy-views-assets && concurrently \"npm:watch-ts\" \"nodemon dist/server.js\"",
    "copy-views-assets": "ts-node copyViewsAssets.ts",
    "ts-node": "ts-node -r tsconfig-paths/register --"
  },
  "author": "Takumi Adachi",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "dotenv": "^8.0.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "nano": "^8.1.0",
    "pug": "^2.0.4",
    "session-file-store": "^1.3.1",
    "shelljs": "^0.8.3",
    "snoowrap": "^1.18.0",
    "typescript": "^3.5.3",
    "uuid": "^3.3.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/runtime": "^7.5.5",
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.0",
    "@types/express-session": "^1.15.13",
    "@types/lodash": "^4.14.136",
    "@types/moment": "^2.13.0",
    "@types/snoowrap": "^1.15.3",
    "@types/uuid": "^3.4.5",
    "@types/winston": "^2.4.4",
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@prettier/plugin-pug": "^1.0.0",
    "@types/jest": "^24.0.18",
    "babel-jest": "^24.8.0",
    "concurrently": "^4.1.2",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "^3.8.0"
  }
}